<!-- PAGINA PRINCIPAL-->
<div class="container">
    <div class="page-header">
        <h1 class="app-title">Rock & Roll <small class="subtitle"> with
            Ember.js</small></h1>

    </div>
    <div class="main-content">
        <ul class="list paO maO">
            {{#each model as |song|}}
            <li>
                {{song.title}} by <em>{{song.band}}</em>
                <span class="rating fr">{{song.rating}}</span>
            </li>
            {{/each}}
        </ul>
    </div>
</div>