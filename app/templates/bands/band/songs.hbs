<ul class="rr-list">
    {{#each model.songs as |song|}}
        <li class="rr-list-item">
            {{song.title}} <span class="rating fr">{{song.rating}}</span>
        </li>
    {{else}}
        <li class="rr-empty-message">
            No {{model.name}} songs yet.<!--</a> -->
        </li>
    {{/each}}
</ul>